@model IEnumerable<VetsJobFinder.Models.Job>




@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create", new { area = "" }, new { @class = "btn createJob" })
</p>
<table id="jobTable" class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PostedOn)
        </th>
             <th>
            @Html.DisplayNameFor(model => model.StartApplicationDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndApplicationDate)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StartApplicationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartApplicationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
